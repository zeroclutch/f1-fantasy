{
    "plugins": [],
    "recurseDepth": 5,
    "source": {
        "include": [ "src" ],
        "exclude": [ "src/index.js" ],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "theme": "simplex"
    },
    "opts": {
        "destination": "./docs/",
        "encoding": "utf8",
        "private": true,
        "recurse": true,
        "template": "./node_modules/ink-docstrap/template",
        "readme": "./README.md"
    }
}